# Exemplo de variáveis de ambiente para o escavador YouTube

# Banco de dados
DB_URL=postgresql://usuario:senha@localhost:5432/oraculo
# Ou defina individualmente:
# DB_HOST=localhost
# DB_NAME=oraculo
# DB_USER=usuario
# DB_PASSWORD=senha
# DB_PORT=5432

# API do YouTube
YOUTUBE_API_KEY=coloque_sua_api_key_aqui

# Canais a serem processados (separados por vírgula)
CHANNEL_IDS=UC123,UC456,UC789
# Ou canal único para retrocompatibilidade
# CHANNEL_ID=UC123

# Parâmetros de processamento
LOTE_TAMANHO=30
PAUSA_ENTRE_VIDEOS=10
PAUSA_ENTRE_LOTES=60
MAX_RETRIES=3
