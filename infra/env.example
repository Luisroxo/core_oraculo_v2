# Exemplo de variáveis de ambiente para o ORÁCULO v2.0
# Copie este arquivo para .env e preencha os valores conforme seu ambiente

# API Gateway
API_GATEWAY_HOST=0.0.0.0
API_GATEWAY_PORT=8000
API_GATEWAY_SECRET_KEY=changeme

# Orquestrador
ORQUESTRADOR_HOST=0.0.0.0
ORQUESTRADOR_PORT=8001

# Banco de Dados
POSTGRES_USER=oraculo
POSTGRES_PASSWORD=oraculo
POSTGRES_DB=oraculo
POSTGRES_HOST=postgres
POSTGRES_PORT=5432

# Redis
REDIS_HOST=redis
REDIS_PORT=6379

# RabbitMQ
RABBITMQ_HOST=rabbitmq
RABBITMQ_PORT=5672
RABBITMQ_DEFAULT_USER=guest
RABBITMQ_DEFAULT_PASS=guest

# Microsserviços Escavadores
YOUTUBE_API_KEY=
BLOG_API_URL=
TELEGRAM_BOT_TOKEN=
INSTAGRAM_API_KEY=
LICITACOES_API_URL=

# Executadores
RELATORIOS_OUTPUT_PATH=/data/relatorios
CONTEUDO_IA_API_KEY=
MIDIAS_SOCIAIS_TOKEN=
AUTOMACOES_CONFIG_PATH=/data/automacoes
ERP_CRM_URL=
